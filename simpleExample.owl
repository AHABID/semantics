@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix diat: <https://ontology.eglobalmark.com/apic#> .
@prefix owl: <http://www.w3.org/2002/07/owl#>.

diat:Beekeeper  rdf:type diat:Beekeeper;
                rdf:type owl:Class ;
                rdfs:subClassOf diat:Person ;
                rdf:type diat:Beekeeper ;
                rdf:type diat:Person ;
                rdfs:label "Beekeeper" .



diat:Person     rdf:type diat:Person ;
                rdf:type owl:Class ;
                rdfs:label "Person" . 
